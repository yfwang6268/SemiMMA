Package: SemiMMA
Title: Semiparametric Multivariate Meta-Analysis
Version: 0.0.0.9000
Authors@R: c(
    person(given = "Yifei", family = "Wang", email ="ywang9@smu.edu", role = c("aut", "cre")),
    person(given = "Lifeng", family = "Lin", email ="lifenglin@arizona.edu", role = "aut"),
    person(given = "Yu-lun", family = "Liu", email ="Yulun.Liu@UTSouthwestern.edu", role = "aut")
